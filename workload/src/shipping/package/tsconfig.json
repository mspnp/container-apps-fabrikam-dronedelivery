{
  "$schema": "https://json.schemastore.org/tsconfig",
  "display": "Node 19",
  "_version": "2.0.0",
  "compilerOptions": {
    "outDir": ".bin",
    "module": "ES2022",
    "target": "ES2022",
    "lib": ["ES2022"],
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "moduleResolution": "node",
    "typeRoots": ["./node_modules/@types"]
  },
  "include": ["src/**/*.ts"],
  "exclude": ["node_modules"]
}
